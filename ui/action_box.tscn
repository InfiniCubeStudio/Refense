script/source = "extends Control

func _ready() -> void:
	position.y = (len(get_tree().get_nodes_in_group(\"messages\")) * 50) + 50
	$animations.animation_finished.connect(Callable(self, \"done\"))

func done(_anim):
	remove_from_group(\"messages\")
	get_tree().call_group_flags(
		SceneTree.GROUP_CALL_DEFERRED, 
		\"messages\", 
		\"message_done\", 
	)
	queue_free()

func message_done():
	position.y -= 50
"

